select pf.park_id, f.fee, p.park_name
from park_fee as pf
join fees as f
on pf.fee_id = f.fee_id
join park as p
on p.park_id = pf.park_id

select * from activity;
select * from fees;
select * from images;
select * from park;
select * from park_activities;
select * from park_fee;
select * from webcam_url;

DROP TABLE webcam_url;
DROP TABLE images;
DROP TABLE park_fee;
DROP TABLE park_species;
DROP TABLE sci_name;
DROP TABLE park_activities;
DROP TABLE activity;
DROP TABLE fees;
DROP TABLE fire;
DROP TABLE park;

